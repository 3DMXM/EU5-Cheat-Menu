##############################
# Cheat Menu Window
##############################

window = {
    name = "cheat_menu_window"
    size = { 650 850 }
    parentanchor = center
    layer = windows_layer
    movable = yes

    state = {
        name = _show
        using = Animation_FadeIn_Standard
    }

    state = {
        name = _hide
        using = Animation_FadeOut_Standard
    }

    # Main window background with styling
    using = bg_window_default_alt
    allow_outside = yes

