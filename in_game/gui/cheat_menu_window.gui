    widget = {
        parentanchor = top|right
        position = { -5 5 }
        size = { 40 40 }

        button_close_alt = {
            size = { 40 40 }

            blockoverride "close_onclick" {
                onclick = "[ExecuteConsoleCommand('gui.ClearWidgets cheat_menu_window')]"
            }

            tooltip = "CLOSE"
        }
    }

