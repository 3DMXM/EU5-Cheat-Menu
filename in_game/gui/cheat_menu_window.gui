##############################
# Cheat Section Header Template
##############################
types cheat_section_header {
    type cheat_section_header = widget {
        layoutpolicy_horizontal = expanding
        size = { 486 66 }
        margin_top = 8
        margin_bottom = 5

        background = {
            using = tile_background_squared_texture
            margin = {-6 -6}

            modify_texture = {
                using = color_dark_blue_texture
            }

            using = overlay_window
            using = overlay_leather

            modify_texture = {
                texture = "gfx/interface/graphical_cultures/european_gfx/top_bar_bg.dds"
                texture_density = 1
                spriteType = corneredtiled
                spriteborder = { 0 0 }
                blend_mode = overlay
            }
        }

        background = {
            texture = "gfx/interface/graphical_cultures/european_gfx/top_bar_sideflavor.dds"
            texture_density = 1.5
            spriteType = Corneredstretched
            spriteborder = { 20 0 }
            alpha = 1
            margin = {-12 -6}

            modify_texture = {
                using = color_intense_bg_blue_texture
                blend_mode = multiply
                alpha = 0.9

            }
            modify_texture = {
                using = color_progress_blue_texture
                blend_mode = overlay

            }
        }

        background = {
            texture = "gfx/interface/graphical_cultures/european_gfx/pause_label_frame.dds"
            texture_density = 2
            spriteType = Corneredstretched
            spriteborder = { 26 0 }
        }

        background = {
            texture = "gfx/interface/graphical_cultures/european_gfx/pause_label_frame_gem.dds"
            texture_density = 2
            spriteType = Corneredstretched
            spriteborder = { 26 0 }
        }

        text_single = {
            # raw_text = "section_title"
            parentanchor = center
            align = center|nobaseline
            using = Font_Type_Headers
            using = Font_Size_Medium
            block "section_title" {
                raw_text = "Section Title"
            }
        }

    }
}

##############################
# Cheat Button Template
##############################
types cheat_button {

    # Compact button for horizontal layout
    type cheat_button_compact = button_regular {

        size = {190 38}
        blockoverride "button_texture" {
            texture = "gfx/interface/buttons/button_wax_blue.dds"
        }

        align = center|nobaseline
        using = Font_Type_Headers

        block "button_text" {
            text = "Compact"
        }

        block "button_action" {
            onclick = ""
        }

        block "button_tooltip" {
            tooltip = ""
        }


    }
}